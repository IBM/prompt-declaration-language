{
    "title": "Things not to forget",
    "prompts": [
        "A list of things not to forget when ",
        "travelling:\n",
        {
            "prompts": [
                "- ",
                {
                    "var": "THING",
                    "constraints": [
                        {
                            "in": [
                                "passport",
                                "phone",
                                "keys"
                            ]
                        },
                        {
                            "words_len": "<=2"
                        }
                    ],
                    "lookup": {
                        "model": "ibm/granite-20b-code-instruct-v1",
                        "decoding": "argmax",
                        "input": "context"
                    }
                },
                "\n"
            ],
            "repeat": 2
        },
        "The most important of these is ",
        {
            "var": "ITEM",
            "lookup": {
                "model": "ibm/granite-20b-code-instruct-v1",
                "decoding": "argmax",
                "input": "context"
            }
        },
        "."
    ]
}