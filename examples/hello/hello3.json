{
    "description": "Hello world with a constraint",
    "prompts": [
        "Hello,",
        {
            "model": "ibm/granite-20b-code-instruct-v1",
            "decoding": "argmax",
            "stop_sequences": [
                "!"
            ],
            "include_stop_sequences": true,
            "constraints": [
                {
                    "words_len": 1
                }
            ]
        },
        "\n"
    ]
}

