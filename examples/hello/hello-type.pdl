# Expected not to type check
description: Hello world with type specification
text:
- def: GEN
  text: "What is the meaning of life"
- def: translate
  function:
    sentence: str
    language: str
  spec: int
  return:
    lastOf:
    - "\nTranslate the sentence '${ sentence }' to ${ language }.\n"
    - model: ollama/granite-code:8b
      parameters:
        stop: ["\n"]
- call: ${ translate }
  spec: str
  args:
    sentence: ${ GEN }
    language: French
- call: ${ translate }
  args:
    sentence: ${ GEN }
    language: Spanish

