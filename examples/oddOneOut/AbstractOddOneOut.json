{
    "description": "Odd one out, abstract version",
    "prompts": [
        "Pick the odd word out: ",
        {
            "var": "OPTIONS",
            "type": "List(String)",
            "separator": ", "
        },
        ".\n",
        {
            "var": "REASONING",
            "constraints": [
                {
                    "not_contains": "\n"
                },
                {
                    "not_contains": "Pick"
                },
                {
                    "words_len": "<40"
                }
            ]
        },
        "\n",
        "So the odd one is ",
        {
            "var": "RESULT",
            "constraints": [
                {
                    "in": "OPTIONS"
                }
            ]
        },
        ".\n\n"
    ],
    "repeat": "*"
}